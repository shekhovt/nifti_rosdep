uri: 'http://www.openni.org/wp-content/uploads/2012/12/OpenNI-Bin-Dev-Linux-x86-v1.5.4.0.tar.zip'
install-script: |
  #!/bin/bash
  pwd
  ls
  sudo ./install.sh
check-presence-script: |
  #!/bin/bash
  if [[ -n $(find /usr/include -name "ni") ]]
  then
    echo "installed"
    exit 0
  else
    echo "not installed"
    exit 1
  fi
exec-path: OpenNI-Bin-Dev-Linux-x64-v1.5.2.23
