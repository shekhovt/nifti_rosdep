uri: 'http://www.openni.org/wp-content/uploads/2012/12/Sensor-Bin-Linux-x64-v5.1.2.1.tar.zip'
install-script: |
  #!/bin/bash
  pwd
  ls
  sudo ./install.sh
check-presence-script: |
  #!/bin/bash
  if [[ -n $(find /usr/etc -name "primesense") ]]
  then
    echo "installed"
    exit 0
  else
    echo "not installed"
    exit 1
  fi
exec-path: Sensor-Bin-Linux-x64-v5.1.0.41
