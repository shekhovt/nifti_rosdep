uri: 'https://raw.github.com/ethz-asl/nifti_rosdep/master/README.md'
check-presence-script: |
  #!/bin/bash
  cd `rosstack find nifti_mapping`
  cd .. 
  if [ -d gpgpu ]
  then
    cd gpgpu
    git pull
    rosdep install gpgpu
    rosmake gpgpu
    exit 1
  else
    exit 0
  fi
install-script: |
  #!/bin/bash
  cd `rosstack find nifti_mapping`
  cd ..
  git clone http://ram.umd.edu/git/ros/gpgpu.git
  rosdep install gpgpu
  rosmake gpgpu
